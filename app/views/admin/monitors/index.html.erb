<% content_for :head do %>
	<%= javascript_include_tag "admin/monitors" %>
<% end %>

<p id="dispatcher-state">
  <span class="running"></span>
</p>

<table class="table table-bordered ">
	<thead>
		<th>Host</th>
		<th>PID</th>
		<th>UUID</th>
		<th>IDLE</th>
		<th>Ack ping</th>
		<th>Run Since</th>
		<th>Callback url</th>
		<th>Blocked</th>
		<th>Stale</th>
	</thead>
	<tbody id="vulcains-states">
	</tbody>
</table>

<audio id="sound-alarm">
  <source src="/alarm.wav" type='audio/x-wav'>
</audio>

<audio id="sound-dog">
  <source src="/dog.mp3" type='audio/x-wav'>
</audio>

<figure id="idleGraph"></figure>
