<% content_for :head do %>
	<%= javascript_include_tag "admin/monitors" %>
<% end %>
<p id="dispatcher-state">
  <span class="running"></span>
</p>
<table class="table table-striped table-bordered ">
	<thead>
		<th>Host</th>
		<th>PID</th>
		<th>UUID</th>
		<th>IDLE</th>
		<th>Ack ping</th>
		<th>Run Since</th>
		<th>Callback url</th>
		<th>Blocked</th>
		<th>Stale</th>
	</thead>
	<tbody id="vulcains-states">
	</tbody>
	
	<audio id="sound-naissance">
    <source src="/naissance.mp3" type='audio/mpeg; codecs="mp3"'>
  </audio>
  
  <audio id="sound-agonie">
    <source src="/agonie.mp3" type='audio/mpeg; codecs="mp3"'>
  </audio>
  
  <audio id="sound-alarm">
    <source src="/alarm.wav" type='audio/x-wav'>
  </audio>
  
	
</table>

<figure id="idleGraph"></figure>
